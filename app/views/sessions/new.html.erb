<h1>Log In</h1>

<%= form_tag sessions_path do %>
  <div class="field center">
    <%= label_tag :email %><br>
    <%= text_field_tag :email %>
  </div>
  <div class="field center">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>
  <div class="center">
    <div>
      <%= submit_tag "Log In" %>
    </div>
  </div>
<% end %>